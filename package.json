{
  "name": "farcaster-survey-bot",
  "version": "1.0.0",
  "description": "An open-source NodeJS bot designed to post engaging surveys directly on Farcaster.",
  "main": "dist/app.js",
  "repository": "https://github.com/ponder-surveys/farcaster-survey-bot",
  "author": "benadamsky <benadamsky@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "tsc-watch --skipLibCheck --onSuccess 'node dist/app.js'",
    "build": "tsc --skipLibCheck",
    "prettier": "prettier --config .prettierrc '*/**/*.{js,ts}' --write",
    "lint": "yarn prettier && eslint --max-warnings 0 --ext ts,tsx,json src"
  },
  "dependencies": {
    "@neynar/nodejs-sdk": "^1.6.1",
    "@supabase/supabase-js": "^2.8.0",
    "@thirdweb-dev/sdk": "^4.0.36",
    "axios": "^1.6.0",
    "cron-parser": "^4.8.1",
    "dotenv": "^16.0.3",
    "ethers": "^5",
    "node-cron": "^3.0.2"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "prettier": "^2.8.3",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.3.1"
}
